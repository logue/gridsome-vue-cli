{
  "name": "gridsome-vue-cli",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "gridsome:build": "gridsome build",
    "gridsome:explore": "gridsome explore",
    "gts:lint": "gts lint ./src/**/*.{js,jsx,ts,tsx,vue}",
    "gts:check": "gts check ./src/**/*.{js,jsx,ts,tsx,vue}",
    "gts:fix": "gts fix ./src/**/*.{js,jsx,ts,tsx,vue}",
    "gts:clean": "gts clean",
    "prepare": "npm.cmd run build",
    "pretest": "npm.cmd run build",
    "posttest": "npm.cmd run lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "gridsome": "^0.7.23",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1"
  },
  "devDependencies": {
    "@gridsome/source-graphql": "^0.1.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint": "^7.18.0",
    "eslint-plugin-gridsome": "^1.6.1",
    "eslint-plugin-vue": "^7.5.0",
    "gridsome-plugin-typescript": "^0.4.0",
    "gts": "^3.1.0",
    "lint-staged": "^10.5.3",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vue-template-compiler": "^2.6.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "vuePlugins": {
    "service": [
      "vue-cli-plugin-gridsome.js"
    ]
  }
}
